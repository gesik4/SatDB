<div class="panel panel-default">
  <div class="panel-body">
    <%= link_to 'Add new satellite', new_satellite_path, class: 'btn btn-primary btn-lg' %>
  </div>
</div>
<table>
  <th>Acronym</th>
  <th>Launch Date</th>
  <th>Inclination, deg</th>
  <th>Dry mass, kg</th>
  <th>Power, W</th>
  <% @satellites.each do |satellite| %>
      <tr>
        <td>
          <%= link_to "#{satellite.acronym}", satellite  %>
        </td>
        <td>
          <%= satellite.launch_date_before_type_cast %>
        </td>
        <td>
          <%= satellite.inclination_before_type_cast %>
        </td>
        <td>
          <%= satellite.mass_dry_before_type_cast %>
        </td>
        <td>
          <%= satellite.power_before_type_cast %>
        </td>
      </tr>
  <% end %>
</table>